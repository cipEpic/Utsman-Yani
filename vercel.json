{
    "version": 2,
    "builds": [
      { "src": "api/*.js", "use": "@vercel/node" },
      { "src": "index.html", "use": "@vercel/static" },
      { "src": "js/*.js", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/messages", "dest": "/api/messages.js" },
      { "src": "/js/(.*)", "dest": "/js/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  